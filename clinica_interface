package br.ifsp.control;

/**
 * Interface 
 * @author Aluno
 */
public interface Sistema {
    
    cadastrarFuncionario();
    exibirFuncionario();
    deletarFuncionario();
    atualizarFuncionario();
    
    cadastrarCliente();
    exibirCliente();
    deletarCliente();
    atualizarCliente();
    
    cadastrarAnimal();
    exibirAnimal();
    deletarAnimal();
    atualizarAnimal();
}
